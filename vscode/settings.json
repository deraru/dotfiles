{
  "[json]": {
    "editor.tabSize": 2,
  },

  "vim.normalModeKeyBindings": [
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": ["<Enter>"],
      "after": ["o", "<ESC>"]

    },
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<ESC>"]
    },
    {
      "before": ["<C-h>"],
      "after": ["<Left>"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<Down>"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<Up>"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<Right>"]
    },
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["v", "v"],
      "after": ["$", "h"]
    },
  ]
}
